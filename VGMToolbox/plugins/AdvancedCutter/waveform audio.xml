<?xml version="1.0" encoding="utf-8"?>
<OffsetFinderTemplate>
	<Header>
		<FormatName>RIFF Waveform Audio File Format</FormatName>
		<Author>涅普缇努</Author>
	</Header>
	<SearchParameters>
		<SearchString>RIFF</SearchString>
		<TreatSearchStringAsHex>false</TreatSearchStringAsHex>
		<UseOffsetString>true</UseOffsetString>
		<OffsetString>WAVEfmt</OffsetString>
		<OffsetCount>8</OffsetCount>
		<SearchStringOffset>0</SearchStringOffset>
		<OutputFileExtension>.wav</OutputFileExtension>
		<CutParameters>
			<CutStyle>offset</CutStyle>
			<CutSizeAtOffset>4</CutSizeAtOffset>
			<CutSizeOffsetSize>4</CutSizeOffsetSize>
			<CutSizeOffsetEndianess>little</CutSizeOffsetEndianess>
		</CutParameters>
		<AddExtraBytes>true</AddExtraBytes>
		<AddExtraByteSize>8</AddExtraByteSize>
	</SearchParameters>
</OffsetFinderTemplate>